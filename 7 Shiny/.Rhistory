##overall goals: Downloadable, filter based on Strength, Set-Size
####Set up####
##libraries
library(shiny)
library(DT)
##load data here
#####Define UI####
ui = fluidPage(
titlePanel("Affordance Norms"),
sidebarLayout(
sidebarPanel(
downloadButton("downloadData", "Download")
)
),
DTOutput('tbl')
)
####Define Server####
server = function(input, output) {
#display table
output$tbl = renderDT(
iris, options = list(lengthChange = FALSE)
)
#download button
output$downloadData = downloadHandler(
filename = function() {
paste(input$dataset, ".csv", sep = "")
},
content = function(file) {
write.csv(datasetInput(), file, row.names = FALSE)
}
)
}
}
####Run the application####
shinyApp(ui = ui, server = server)
